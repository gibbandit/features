{
    "id": "mongodb-tools",
    "version": "1.0.0",
    "name": "MongoDB tools",
    "description": "Installs [MongoDB CLI tools](https://www.mongodb.com/try/download/database-tools)",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest",
                "6.0",
                "5.0",
                "4.4",
                "4.3",
                "4.2",
                "1.0",
                "0.9"
            ],
            "default": "latest",
            "description": "Select or enter a MongoDB tools version."
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "mongodb.mongodb-vscode"
            ]
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}